{$APPTYPE CONSOLE}

var
  t,i:longint;
  x,n:extended;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(n);

      if (n=0) or (n=1) or (n=2) or (n=3) then x:=0;

      if n>=3 then x:=(sqrt(4*(n+1))-2)/2;

      writeln('Case ',i,': ',x:0:0);
    end;

  readln;
end.
 