{$APPTYPE CONSOLE}

var
  t,i,a,b,c,j,f,l,s,d,n:integer;
  x:array [1..1000] of integer;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(f,l,s);

      a:=l-3*f;
      b:=2*s+9*f-5*l;
      c:=-4*s;

      n:=(-b+sqrt(b*b+4*a*c)) / (2*a);

      writeln(n);

      {for j:=1 to n do
        begin
          write(x[j]);
        end;}
    end;

  readln;
end.
