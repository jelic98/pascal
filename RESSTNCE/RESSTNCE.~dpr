{$APPTYPE CONSOLE}

var
  t,i,m,n,x,y:longint;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(n,m);

      x:=;

      y:=;

      writeln(x,'/',y);
    end;

  readln;
end.
