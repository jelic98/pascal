{$APPTYPE CONSOLE}

var
  lj,p,a,b,c,d,e,a1,b1,c1,d1,e1,x:integer;

begin
  readln(lj,p);
  readln(a,b,c,d,e);
  x:=lj*p;
  a1:=a-x;
  b1:=b-x;
  c1:=c-x;
  d1:=d-x;
  e1:=e-x;
  writeln(a1,' ',b1,' ',c1,' ',d1,' ',e1);
  readln;
end.
=======
{$APPTYPE CONSOLE}

var
  lj,p,a,b,c,d,e,a1,b1,c1,d1,e1,x:integer;

begin
  readln(lj,p);
  readln(a,b,c,d,e);
  x:=lj*p;
  a1:=a-x;
  b1:=b-x;
  c1:=c-x;
  d1:=d-x;
  e1:=e-x;
  writeln(a1,' ',b1,' ',c1,' ',d1,' ',e1);
  readln;
end.
