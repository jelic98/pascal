{$APPTYPE CONSOLE}

var
  j,i,t:longint;
  a,b,stepen:real;
  int:int64;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(a,b);

      stepen:=exp(b*ln(abs(a)));

      int:=trunc(stepen);

      writeln(int mod 10);
    end;

  readln;
end.
