{$APPTYPE CONSOLE}

var
  fact:int64;
  t,n,i,j:longint;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(n);

      fact:=1;

      for j:=n downto 1 do
        begin
          fact:=fact*j;
        end;

      writeln(fact);
    end;

  readln;
end.
=======
{$APPTYPE CONSOLE}

var
  fact:int64;
  t,n,i,j:longint;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(n);

      fact:=1;

      for j:=n downto 1 do
        begin
          fact:=fact*j;
        end;

      writeln(fact);
    end;

  readln;
end.
