{$APPTYPE CONSOLE}

var
  s:string;
  i,j:longint;
  t,n,fact,duzina:int64;

begin
  readln(t);

  for i:=1 to t do
    begin
      readln(n);

      fact:=1;

      for j:=n downto 1 do
        begin
          fact:=fact*j;
        end;

      Str(fact,s);

      duzina:=length(s);

      writeln(duzina);

    end;

  readln;
end.
