{$APPTYPE CONSOLE}

var
  s,s1:string;
  i,duzina,sec:integer;
  num:array [1..1000] of integer;

begin
  readln(s);

  sec:=0;

  duzina:=length(s);

  for i:=1 to duzina do
    begin
      num[i]:=ord(s[i])-64;
    end;

  for i:=1 to duzina do
    begin
      if (num[i]=1) or (num[i]=2) or (num[i]=3) then sec:=sec+3;
      if (num[i]=4) or (num[i]=5) or (num[i]=6) then sec:=sec+4;
      if (num[i]=7) or (num[i]=8) or (num[i]=9) then sec:=sec+5;
      if (num[i]=10) or (num[i]=11) or (num[i]=12) then sec:=sec+6;
      if (num[i]=13) or (num[i]=14) or (num[i]=15) then sec:=sec+7;
      if (num[i]=16) or (num[i]=17) or (num[i]=18) or (num[i]=19) then sec:=sec+8;
      if (num[i]=20) or (num[i]=21) or (num[i]=22) then sec:=sec+9;
      if (num[i]=23) or (num[i]=24) or (num[i]=25) or (num[i]=26) then sec:=sec+10;
    end;

  writeln(sec);

  readln;
end.
